<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>


<BorderPane prefHeight="450.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.ReviewSceneController">
   <center>
      <VBox alignment="CENTER">
         <children>
            <FlowPane alignment="CENTER" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <children>
                  <Label fx:id="frontLabel" text="Front of Card" />
               </children>
            </FlowPane>
            <FlowPane alignment="CENTER" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <children>
                  <Label fx:id="backLabel" text="Back of Card" />
               </children>
            </FlowPane>
         </children>
      </VBox>
   </center>
   <bottom>
      <AnchorPane prefHeight="40.0">
         <children>
            <FlowPane fx:id="reviewControlsPane" alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
            <FlowPane alignment="CENTER" maxWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onEndSessionButtonPressed" prefWidth="80.0" text="End Session" />
               </children>
            </FlowPane>
         </children>
      </AnchorPane>
   </bottom>
</BorderPane>
