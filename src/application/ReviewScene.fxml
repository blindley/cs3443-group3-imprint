<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>


<BorderPane prefHeight="450.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ReviewSceneController">
   <center>
      <VBox alignment="CENTER">
         <children>
            <FlowPane alignment="CENTER" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <children>
                  <Label fx:id="frontLabel" text="Front of Card" />
               </children>
            </FlowPane>
            <FlowPane alignment="CENTER" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <children>
                  <Label fx:id="backLabel" text="Back of Card" />
               </children>
            </FlowPane>
         </children>
      </VBox>
   </center>
   <bottom>
      <FlowPane alignment="CENTER" prefHeight="40.0">
         <children>
            <Button fx:id="passButton" mnemonicParsing="false" onAction="#onPassButtonPressed" prefWidth="80.0" text="Pass" />
            <Button fx:id="failButton" mnemonicParsing="false" onAction="#onFailButtonPressed" prefWidth="80.0" text="Fail" />
            <Button fx:id="flipButton" mnemonicParsing="false" onAction="#onFlipButtonPressed" prefWidth="80.0" text="Flip" />
         </children>
      </FlowPane>
   </bottom>
</BorderPane>
